{
  "name": "hummingbird",
  "private": true,
  "version": "0.1.0",
  "description": "A full-stack web application for analyzing machine learning data and identifying unique entities in that data.",
  "source": "index.html",
  "scripts": {
    "fire-all-engines": "npm run build && npm run emulators",
    "dev": "vite",
    "build": "tsc && vite build --dev",
    "preview": "vite preview",
    "emulators": "firebase emulators:start",
    "emulators:dev": "firebase emulators:start --only auth,functions,firestore,pubsub,storage",
    "check": "tsc --noEmit",
    "lint": "eslint --ext js,ts,jsx,tsx src",
    "lint:fix": "eslint --ext js,ts,jsx,tsx src --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Midlight25/hummingbird"
  },
  "keywords": [
    "react",
    "react-app",
    "vite",
    "typescript",
    "web-app",
    "progressive-web-app",
    "firebase"
  ],
  "authors": [
    "Sean Holland",
    "Jordan Burgher",
    "Michael Mesquita",
    "Kelly Mylett"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Midlight25/hummingbird/issues"
  },
  "homepage": "https://github.com/Midlight25/hummingbird#readme",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.8.7",
    "firebase": "^9.8.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-firebase-hooks": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase-tools": "^11.2.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "engines": {
    "node": ">=16"
  }
}
